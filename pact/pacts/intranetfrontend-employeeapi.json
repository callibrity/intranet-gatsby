{
  "consumer": {
    "name": "IntranetFrontend"
  },
  "provider": {
    "name": "EmployeeAPI"
  },
  "interactions": [
    {
      "description": "get requested employees details",
      "providerState": "Employee Exists",
      "request": {
        "method": "GET",
        "path": "/employee"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "first_name": "Collin",
          "last_name": "Johnson",
          "role": "Account Manager"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}